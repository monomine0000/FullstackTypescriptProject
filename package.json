{
  "name": "fullstack-typescript",
  "version": "0.0.1",
  "description": "Example project using TypeScript for both Backend and Frontend code",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev:backend": "ts-node-dev --respawn --transpileOnly ./src/backend/index.ts",
    "dev:frontend": "tsc --watch",
    "start": "npm run build && node ./dist/backend/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "fullstack",
    "typescript"
  ],
  "author": "kmturley",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.7.4"
  }
}
